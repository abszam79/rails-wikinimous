<div class="container">
  <div class="row">
    <h1>Wikinimous</h1>
    <% @articles.each do |article| %>
      <div class="articles">
        <p><%= link_to article.title, article_path(article) %></p>
      </div>
    <% end %>
    <div class="btn btn-warning">
    <p><%= link_to "Add a new article", new_article_path %></p>
    </div>
  </div>
</div>


